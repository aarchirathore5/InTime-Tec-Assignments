#include <stdio.h>
#include <stdlib.h>

int main()
{
    int arraySize;
    int *arrayPointer;
    int position;

    scanf("%d", &arraySize);

    arrayPointer = (int*) malloc(arraySize * sizeof(int));

    for(position = 0; position < arraySize; position++) {
        scanf("%d", arrayPointer + position);
    }

    int *startPointer = arrayPointer;
    int *endPointer = arrayPointer + arraySize - 1;

    while(startPointer < endPointer) {
        int temporaryValue = *startPointer;
        *startPointer = *endPointer;
        *endPointer = temporaryValue;

        startPointer++;
        endPointer--;
    }

    for(position = 0; position < arraySize; position++) {
        printf("%d ", *(arrayPointer + position));
    }

    free(arrayPointer);
    return 0;
}
