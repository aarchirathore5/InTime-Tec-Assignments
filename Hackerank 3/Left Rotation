int* rotateLeft(int rotationCount, int arrayLength, int* array, int* result_count) {
    *result_count = arrayLength;

    int* rotatedArray = malloc(arrayLength * sizeof(int));

    for (int position = 0; position < arrayLength; position++) {
        int newPosition = (position + arrayLength - rotationCount) % arrayLength;
        *(rotatedArray + newPosition) = *(array + position);
    }

    return rotatedArray;
}
